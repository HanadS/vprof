[MESSAGES CONTROL]
disable=exec-used, wrong-import-order, fixme,
        invalid-name, no-member, too-many-arguments, too-many-locals, duplicate-code
        # Disable duplicate-code globally, since can't disable locally
        # https://github.com/PyCQA/pylint/issues/214
[FORMAT]

# Maximum number of characters on a single line.
max-line-length=80
